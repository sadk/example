<?xml version="1.0" encoding="UTF-8"?>
<statements namespace="org.lsqt.sys.model.RegionDistrict" cache="true" import="org.lsqt.components.util.collection.ArrayUtil,org.lsqt.components.util.lang.StringUtil" >
	<table name="s_district">
		<column name="DistrictID" property="id"/>
		<column name="DistrictName" property="name"/>
		<column name="CityID" property="cityId"/>
	</table>

	<statement id="getAll" >
		<![CDATA[
			select * from s_district
		]]>
	</statement>
</statements>
