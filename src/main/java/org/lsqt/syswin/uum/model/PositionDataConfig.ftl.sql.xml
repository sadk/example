<?xml version="1.0" encoding="UTF-8"?>
<statements namespace="org.lsqt.syswin.uum.model.PositionDataConfig" cache="true" import="org.lsqt.components.util.collection.ArrayUtil,org.lsqt.components.util.lang.StringUtil" >
	<table name="t_power_duties_config">
			<column id="id" type="auto" property="id" text="主键" />
	 	 
	 		<column property="positionId" name="duties_id" text="岗位ID"/>
	 		
	 		<column property="colType" name="col_type" text="授权类型:mySelf=本人 underMe=直属下级 all=全部"/>
	 		
	 		<column property="orgId" name="org_id" text="组织机构Id"/>
	 	
	 		<column property="orgPid" name="org_pid" text="组织机构父Id"/>
	 	
	 		<column property="orgType" name="org_type" text="组织机构类型"/>
	 	
	 		<column property="orgName" name="org_name" text="组织机构名称"/>
	 		
	</table>
		
	<statement id="queryForPage" parameterName="query">
	 	<![CDATA[
	 		select * from t_power_duties_config where 1=1
		 	<#if query??>
				<#if query.positionId??> and duties_id in (${query.positionId}) </#if>
				
			</#if>
	 	]]>
	</statement>
</statements>
